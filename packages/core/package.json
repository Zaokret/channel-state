    "format": "prettier --write 'src/**/*.{ts,tsx}' --config ../../.prettierrc.json",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
